{
    "manifest_version": 3,
    "name": "Dark Mode Extension for virtualmanager",
    "version": "1.0",
    "action": { "default_popup": "popup.html"},
    "background": {
      "service_worker": "background.js"},
    "permissions": ["tabs", "storage","scripting"],
    "host_permissions": ["*://*.virtualmanager.com/*"]
  }
  